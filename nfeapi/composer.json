    "type": "project",
    "require": {
        "php": ">=7.4",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.5",
        "php-di/php-di": "^6.4",
        "monolog/monolog": "^2.8",
        "guzzlehttp/guzzle": "^7.5",
        "firebase/php-jwt": "^6.4",
        "vlucas/phpdotenv": "^5.4",
